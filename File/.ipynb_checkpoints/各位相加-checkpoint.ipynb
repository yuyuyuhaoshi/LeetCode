   "source": [
    "class Solution:\n",
    "    def addDigits(self, num: int) -> int:\n",
    "        num_list = [int(i) for i in str(num)]\n",
    "        \n",
    "        n = sum(num_list)\n",
    "        \n",
    "        while len(str(n)) > 1:\n",
    "            n = self.addDigits(n)\n",
    "        return n\n",
    "            "
   ]
