   "source": [
    "class Solution:\n",
    "    def constructRectangle(self, area: int) -> List[int]:\n",
    "        w = int(math.sqrt(area))\n",
    "\n",
    "        while area % w != 0:\n",
    "            w -= 1\n",
    "        return [int(area/w) , w]"
   ]
