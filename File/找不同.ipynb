   "source": [
    "class Solution:\n",
    "    def findTheDifference(self, s: str, t: str) -> str:\n",
    "\n",
    "        a = s + t\n",
    "        result = 0\n",
    "        for i in a:\n",
    "            result ^= ord(i)\n",
    "        return chr(result)"
   ]
  }
